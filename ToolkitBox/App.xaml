<Application
    x:Class="ToolkitBox.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:config="https://blame/fluentwpf/2023/configs"
    xmlns:local="clr-namespace:ToolkitBox"
    xmlns:theme="clr-namespace:FluentWPF;assembly=FluentWPF">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <theme:FluentXamlResource Type="Dark" />
                <ResourceDictionary Source="pack://application:,,,/FluentWPF;component/FluentXamlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
